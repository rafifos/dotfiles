[
  {
    "clean": [
      "$HOME",
      "$HOME/.config/zsh"
    ]
  },
  {
    "link": {
      "$HOME/.zshenv": {
        "create": true,
        "path": "zshim"
      },
      "$HOME/.config/zsh/.zlogin": {
        "create": true,
        "path": "zlogin"
      },
      "$HOME/.config/zsh/.zlogout": {
        "create": true,
        "path": "zlogout"
      },
      "$HOME/.config/zsh/.zprofile": {
        "create": true,
        "path": "zprofile"
      },
      "$HOME/.config/zsh/.zshenv": {
        "create": true,
        "path": "zshenv"
      },
      "$HOME/.config/zsh/.zshrc": {
        "create": true,
        "path": "zshrc"
      },
      "$HOME/.config/zsh/functions": {
        "create": true,
        "path": "functions"
      },
      "$HOME/.local/share/zinit/plugins/_local---config": {
        "create": true,
        "path": "plugins/config"
      },
      "$HOME/.local/share/zinit/plugins/_local---docker": {
        "create": true,
        "path": "plugins/docker"
      },
      "$HOME/.local/share/zinit/plugins/_local---exa": {
        "create": true,
        "path": "plugins/exa"
      },
      "$HOME/.local/share/zinit/plugins/_local---fzf": {
        "create": true,
        "path": "plugins/fzf"
      },
      "$HOME/.local/share/zinit/plugins/_local---gpg": {
        "create": true,
        "path": "plugins/gpg"
      },
      "$HOME/.local/share/zinit/plugins/_local---httpie": {
        "create": true,
        "path": "plugins/httpie"
      },
      "$HOME/.local/share/zinit/plugins/_local---private": {
        "create": true,
        "path": "plugins/private"
      },
      "$HOME/.local/share/zinit/plugins/_local---rip": {
        "create": true,
        "path": "plugins/rip"
      },
      "$HOME/.local/share/zinit/plugins/_local---ripgrep": {
        "create": true,
        "path": "plugins/ripgrep"
      },
      "$HOME/.local/share/zinit/plugins/_local---rsync": {
        "create": true,
        "path": "plugins/rsync"
      }
    }
  }
]
