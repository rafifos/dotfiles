[
  {
    "clean": [
      "$HOME/.config/gnupg"
    ]
  },
  {
    "link": {
      "$HOME/.config/gnupg/gpg-agent.conf": {
        "create": true,
        "path": "gpg-agent.conf"
      },
      "$HOME/.config/gnupg/gpg.conf": {
        "create": true,
        "path": "gpg.conf"
      }
    }
  }
]
