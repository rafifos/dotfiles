{
  "[markdown]": {
    "editor.wordWrap": "wordWrapColumn",
    "editor.wordWrapColumn": 120
  },
  "better-comments.tags": [
    {
      "backgroundColor": "transparent",
      "color": "#4C566A",
      "strikethrough": true,
      "tag": "//"
    },
    {
      "backgroundColor": "transparent",
      "color": "#88C0D0",
      "strikethrough": false,
      "tag": "*"
    },
    {
      "backgroundColor": "transparent",
      "color": "#8FBCBB",
      "strikethrough": false,
      "tag": "todo"
    },
    {
      "backgroundColor": "transparent",
      "color": "#B48EAD",
      "strikethrough": false,
      "tag": "?"
    },
    {
      "backgroundColor": "transparent",
      "color": "#BF616A",
      "strikethrough": false,
      "tag": "!"
    }
  ],
  "color-highlight.markRuler": false,
  "debug.inlineValues": true,
  "debug.toolBarLocation": "docked",
  "editor.copyWithSyntaxHighlighting": false,
  "editor.cursorBlinking": "smooth",
  "editor.cursorSmoothCaretAnimation": true,
  "editor.dragAndDrop": false,
  "editor.fontFamily": "Iosevka",
  "editor.fontLigatures": true,
  "editor.fontSize": 16,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.minimap.renderCharacters": false,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.renderWhitespace": "boundary",
  "editor.rulers": [
    120,
    80
  ],
  "editor.smoothScrolling": true,
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "eslint.autoFixOnSave": true,
  "eslint.packageManager": "yarn",
  "files.associations": {
    "zimrc": "shellscript",
    "zlogin": "shellscript",
    "zlogout": "shellscript",
    "zshenv": "shellscript",
    "zshrc": "shellscript",
    "ztoken": "shellscript"
  },
  "files.autoSave": "onFocusChange",
  "files.simpleDialog.enable": true,
  "git.branchSortOrder": "alphabetically",
  "git.defaultCloneDirectory": "$HOME/WorkDir",
  "git.enableCommitSigning": true,
  "git.inputValidationLength": 80,
  "git.inputValidationSubjectLength": null,
  "git.scanRepositories": [
    "$HOME/WorkDir"
  ],
  "git.showPushSuccessNotification": true,
  "git.supportCancellation": true,
  "git.untrackedChanges": "separate",
  "gitlens.blame.avatars": false,
  "gitlens.blame.heatmap.enabled": false,
  "gitlens.blame.highlight.locations": [
    "gutter"
  ],
  "gitlens.codeLens.enabled": false,
  "gitlens.currentLine.enabled": false,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.hovers.enabled": false,
  "gitlens.recentChanges.highlight.locations": [
    "gutter",
    "line",
    "overview"
  ],
  "gitlens.views.compare.enabled": false,
  "gitlens.views.fileHistory.location": "scm",
  "gitlens.views.lineHistory.enabled": false,
  "gitlens.views.repositories.enabled": false,
  "gitlens.views.search.enabled": false,
  "glean.experiments": [
    "hooksForFunctionalComponents"
  ],
  "javascript.updateImportsOnFileMove.enabled": "always",
  "jest.autoEnable": false,
  "material-icon-theme.activeIconPack": "react_redux",
  "material-icon-theme.folders.color": "#616e88",
  "material-icon-theme.hidesExplorerArrows": true,
  "material-icon-theme.saturation": 0.6,
  "merge-conflict.autoNavigateNextConflict.enabled": true,
  "npm.packageManager": "yarn",
  "terminal.external.osxExec": "iTerm2.app",
  "terminal.integrated.cursorBlinking": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "workbench.colorTheme": "Nord",
  "workbench.editor.enablePreview": false,
  "workbench.fontAliasing": "antialiased",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.settings.useSplitJSON": true,
  "workbench.startupEditor": "readme"
}