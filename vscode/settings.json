{
  "[javascript]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    },
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.wordWrap": "wordWrapColumn",
    "editor.wordWrapColumn": 80
  },
  "color-highlight.markRuler": false,
  "debug.inlineValues": true,
  "debug.openExplorerOnEnd": true,
  "debug.showSubSessionsInToolBar": true,
  "debug.toolBarLocation": "docked",
  "devskim.enableBestPracticeRules": true,
  "devskim.enableManualReviewRules": true,
  "devskim.manualReviewerName": "Rafael Julio <hello.rjulio@gmail.com>",
  "devskim.removeFindingsOnClose": true,
  "editor.cursorBlinking": "smooth",
  "editor.cursorSmoothCaretAnimation": true,
  "editor.dragAndDrop": false,
  "editor.fontFamily": "Iosevka",
  "editor.fontLigatures": true,
  "editor.fontSize": 16,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.minimap.renderCharacters": false,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.renderWhitespace": "boundary",
  "editor.rulers": [
    120,
    80
  ],
  "editor.smoothScrolling": true,
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "eslint.autoFixOnSave": true,
  "eslint.lintTask.enable": true,
  "eslint.packageManager": "yarn",
  "extensions.showRecommendationsOnlyOnDemand": true,
  "files.associations": {
    "zimrc": "shellscript",
    "zlogin": "shellscript",
    "zlogout": "shellscript",
    "zshenv": "shellscript",
    "zshrc": "shellscript",
    "ztoken": "shellscript"
  },
  "files.autoSave": "onFocusChange",
  "files.simpleDialog.enable": true,
  "git.alwaysShowStagedChangesResourceGroup": true,
  "git.branchSortOrder": "alphabetically",
  "git.defaultCloneDirectory": "$HOME/WorkDir",
  "git.enableCommitSigning": true,
  "git.inputValidationLength": 80,
  "git.inputValidationSubjectLength": null,
  "git.scanRepositories": [
    "$HOME/WorkDir"
  ],
  "git.showPushSuccessNotification": true,
  "git.supportCancellation": true,
  "gitlens.blame.avatars": false,
  "gitlens.blame.heatmap.enabled": false,
  "gitlens.blame.highlight.locations": [
    "gutter"
  ],
  "gitlens.codeLens.enabled": false,
  "gitlens.currentLine.enabled": false,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.hovers.enabled": false,
  "gitlens.recentChanges.highlight.locations": [
    "gutter",
    "line",
    "overview"
  ],
  "gitlens.views.compare.enabled": false,
  "gitlens.views.fileHistory.location": "scm",
  "gitlens.views.lineHistory.enabled": false,
  "gitlens.views.repositories.enabled": false,
  "gitlens.views.search.enabled": false,
  "glean.experiments": [
    "hooksForFunctionalComponents"
  ],
  "javascript.format.enable": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "javascript.validate.enable": false,
  "jest.autoEnable": false,
  "licenser.author": "Rafael Julio",
  "licenser.license": "MIT",
  "licenser.useSingleLineStyle": false,
  "material-icon-theme.activeIconPack": "react_redux",
  "material-icon-theme.folders.color": "#616E88",
  "material-icon-theme.hidesExplorerArrows": true,
  "material-icon-theme.saturation": 0.6,
  "material-icon-theme.showUpdateMessage": true,
  "merge-conflict.autoNavigateNextConflict.enabled": true,
  "npm.packageManager": "yarn",
  "search.maintainFileSearchCache": true,
  "sortLines.sortEntireFile": true,
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "terminal.external.osxExec": "iTerm2.app",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.experimentalRefreshOnResume": true,
  "terminal.integrated.experimentalUseTitleEvent": true,
  "terminal.integrated.rightClickBehavior": "default",
  "tslint.alwaysShowRuleFailuresAsWarnings": false,
  "tslint.packageManager": "yarn",
  "typescript.format.enable": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "typescript.validate.enable": false,
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "window.restoreFullscreen": true,
  "workbench.colorTheme": "Nord",
  "workbench.editor.enablePreview": false,
  "workbench.enableExperiments": false,
  "workbench.fontAliasing": "antialiased",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.settings.enableNaturalLanguageSearch": false,
  "workbench.settings.useSplitJSON": true,
  "workbench.startupEditor": "readme"
}