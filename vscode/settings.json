{
  "[markdown]": {
    "editor.wordWrap": "wordWrapColumn",
    "editor.wordWrapColumn": 80
  },
  "[javascript]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    },
    "editor.formatOnSaveTimeout": 1000
  },
  "color-highlight.markRuler": false,
  "debug.inlineValues": true,
  "debug.node.autoAttach": "on",
  "debug.openExplorerOnEnd": true,
  "debug.showSubSessionsInToolBar": true,
  "debug.toolBarLocation": "docked",
  "devskim.enableBestPracticeRules": true,
  "devskim.enableManualReviewRules": true,
  "devskim.manualReviewerName": "Rafael Julio <hello.rjulio@gmail.com>",
  "editor.cursorBlinking": "smooth",
  "editor.cursorSmoothCaretAnimation": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.dragAndDrop": false,
  "editor.fontFamily": "Iosevka",
  "editor.fontLigatures": true,
  "editor.fontSize": 16,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.minimap.renderCharacters": false,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.renderWhitespace": "boundary",
  "editor.rulers": [
    120,
    80
  ],
  "editor.smoothScrolling": true,
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "eslint.autoFixOnSave": true,
  "eslint.experimental.incrementalSync": true,
  "eslint.lintTask.enable": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "extensions.showRecommendationsOnlyOnDemand": true,
  "files.associations": {
    "zimrc": "shellscript",
    "zlogin": "shellscript",
    "zlogout": "shellscript",
    "zshenv": "shellscript",
    "zshrc": "shellscript",
    "ztoken": "shellscript"
  },
  "files.autoSave": "onFocusChange",
  "files.simpleDialog.enable": true,
  "git.alwaysShowStagedChangesResourceGroup": true,
  "git.branchSortOrder": "alphabetically",
  "git.defaultCloneDirectory": "$HOME/WorkDir",
  "git.enableCommitSigning": true,
  "git.inputValidationLength": 80,
  "git.inputValidationSubjectLength": null,
  "git.scanRepositories": [
    "$HOME/WorkDir"
  ],
  "git.showPushSuccessNotification": true,
  "git.supportCancellation": true,
  "git.untrackedChanges": "separate",
  "gitlens.blame.avatars": false,
  "gitlens.blame.heatmap.enabled": false,
  "gitlens.blame.highlight.locations": [
    "gutter"
  ],
  "gitlens.codeLens.enabled": false,
  "gitlens.currentLine.enabled": false,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.hovers.enabled": false,
  "gitlens.recentChanges.highlight.locations": [
    "gutter",
    "line",
    "overview"
  ],
  "gitlens.views.compare.enabled": false,
  "gitlens.views.fileHistory.location": "scm",
  "gitlens.views.lineHistory.enabled": false,
  "gitlens.views.repositories.enabled": false,
  "gitlens.views.search.enabled": false,
  "javascript.format.enable": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "licenser.author": "Rafael Julio",
  "licenser.license": "MIT",
  "licenser.useSingleLineStyle": false,
  "merge-conflict.autoNavigateNextConflict.enabled": true,
  "npm-intellisense.scanDevDependencies": true,
  "search.maintainFileSearchCache": true,
  "task.autoDetect": "off",
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "terminal.external.osxExec": "iTerm2.app",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.experimentalRefreshOnResume": true,
  "terminal.integrated.experimentalUseTitleEvent": true,
  "terminal.integrated.rightClickBehavior": "default",
  "typescript.format.enable": false,
  "typescript.surveys.enabled": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "window.restoreFullscreen": true,
  "workbench.colorTheme": "Monokai Pro",
  "workbench.editor.enablePreview": false,
  "workbench.enableExperiments": false,
  "workbench.fontAliasing": "antialiased",
  "workbench.iconTheme": "Monokai Pro Icons",
  "workbench.settings.enableNaturalLanguageSearch": false,
  "workbench.settings.useSplitJSON": true,
  "workbench.startupEditor": "readme"
}